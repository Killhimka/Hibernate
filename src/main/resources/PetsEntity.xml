<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.example.PetsEntity">
        <table name="pets" schema="pets" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
